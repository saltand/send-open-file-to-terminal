{
  "publisher": "saltand",
  "name": "send-open-file-to-terminal",
  "displayName": "Send Open File To Terminal",
  "version": "0.0.4",
  "private": false,
  "packageManager": "pnpm@10.4.1",
  "description": "Send active file path to VSCode terminal quickly",
  "author": "saltand <u6a69a9@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saltand/send-open-file-to-terminal"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "send-open-file-to-terminal.sendActiveFile",
        "title": "Send Active File Path To Terminal",
        "category": "Send Path"
      },
      {
        "command": "send-open-file-to-terminal.copyActiveFile",
        "title": "Copy Active File Path To Clipboard",
        "category": "Send Path"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Send Open File To Terminal",
      "properties": {}
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp && nr publish",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "pnpm": "^10.4.1",
    "reactive-vscode": "^0.2.10",
    "tsdown": "^0.9.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vscode-ext-gen": "^1.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}